  issn = {0967-070X},
  shorttitle = {School Travel Mode Choice and the Characteristics of the Urban Built Environment},
  doi = {10.1016/j.tranpol.2014.10.011},
  abstract = {As observed in several previous studies, the nature of the urban structure can affect children's mode of transportation to school. In this paper, we identify and investigate, in the Finnish context, the elements of the urban structure around homes and en route to school that promote children's ability to walk or cycle to school, using the conceptual domains proposed by Mitra (2013) to frame the work.

