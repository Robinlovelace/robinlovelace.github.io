@article{bernot_structure_2008,
  title = {The Structure of Branched Transportation Networks},
  volume = {32},
  issn = {09442669},
  doi = {10.1007/s00526-007-0139-0},
  abstract = {The transportation problem can be formalized as the problem of finding\$$\backslash$backslash\$nthe optimal paths to transport a measure mu (+) onto a measure mu(-)\$$\backslash$backslash\$nwith the same mass. In contrast with the Monge-Kantorovich\$$\backslash$backslash\$nformalization, recent approaches model the branched structure of such\$$\backslash$backslash\$nsupply networks by an energy functional whose essential feature is to\$$\backslash$backslash\$nfavor wide roads. Given a flow s in a road or a tube or a wire, the\$$\backslash$backslash\$ntransportation cost per unit length is supposed to be proportional to s\$$\backslash$backslash\$n(alpha) with 0 $<$ alpha $<$ 1. For the Monge-Kantorovich energy alpha= 1 so\$$\backslash$backslash\$nthat it is equivalent to have two roads with flow 1/2 or a larger one\$$\backslash$backslash\$nwith flow 1. If instead 0 $<$ alpha $<$ 1, a road with flow s(1)+s(2) is\$$\backslash$backslash\$npreferable to two individual roads s (1) and s (2) because (s(1)+s(2) )\$$\backslash$backslash\$n$<$ s(1)(alpha)+s(2)(alpha) . Thus, this very simple model intuitively\$$\backslash$backslash\$nleads to branched transportation structures. Such a branched structure\$$\backslash$backslash\$nis observable in ground transportation networks, in draining and\$$\backslash$backslash\$nirrigation systems, in electric power supply systems and in natural\$$\backslash$backslash\$nobjects like the blood vessels or the trees. When alpha $>$ 1-1/N such\$$\backslash$backslash\$nstructures can irrigate a whole bounded open set of R-N . The aim of\$$\backslash$backslash\$nthis paper is to give a mathematical proof of several structure and\$$\backslash$backslash\$nregularity properties empirically observed in transportation networks.\$$\backslash$backslash\$nIt is first proven that optimal transportation networks have a tree\$$\backslash$backslash\$nstructure and can be monotonically approximated by finite graphs. An\$$\backslash$backslash\$ninterior regularity result is then proven according to which an optimal\$$\backslash$backslash\$nnetwork is a finite graph away from the irrigated measure. It is also\$$\backslash$backslash\$nproven that the branching number of optimal networks has everywhere a\$$\backslash$backslash\$nuniversal explicit bound. These results answer questions raised in two\$$\backslash$backslash\$nrecent papers by Xia.},
  number = {3},
  journal = {Calculus of Variations and Partial Differential Equations},
  author = {Bernot, Marc and Caselles, Vicent and Morel, Jean-Michel Michel},
  month = nov,
  year = {2008},
  pages = {279--317}
}

@article{brude_models_1993,
  title = {Models for Predicting Accidents at Junctions Where Pedestrians and Cyclists Are Involved. {{How}} Well Do They Fit?},
  volume = {25},
  issn = {0001-4575},
  doi = {10.1016/0001-4575(93)90001-D},
  abstract = {The coefficient of determination, R2, i.e. the squared correlation coefficient between observed and fitted values, is often used as a measure of how well a model predicts the number of accidents at road junctions, for instance. The purpose of this article is to show that the R2 values obtained in different studies are rarely comparable with each other and that a prediction model can be ``nearly perfect'' even if the coefficient of determination is small. Another purpose of the article is to present some results of interest from a practical viewpoint in regard to accidents where pedestrians and cyclists are involved. Empirical R2 values for models predicting accidents at junctions where pedestrians or cyclists are involved are compared with the maximal R2 values that could possibly be obtained. The latter can be calculated both theoretically and with the aid of simulation. How the maximal R2 value depends on the average accident level and the relative dispersion of the expected values for the studied junctions is also shown theoretically. The results obtained show how difficult it can be to determine whether and how far the number of accidents is influenced by additional factors, over and above the traffic flows, which describe the design in greater detail.},
  number = {5},
