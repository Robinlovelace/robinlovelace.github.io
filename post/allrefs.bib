  author = {Poslad, Stefan and Ma, Athen and Wang, Zhenchen and Mei, Haibo},
  month = jun,
  year = {2015},
  keywords = {rewards,intrinsic motivation,cities,framework,feedback,smart cities,mobility behaviour,system,incentives,networks,intelligent transport system},
