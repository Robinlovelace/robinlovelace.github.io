  keywords = {lightweighting,d-86899 landsberg,recycling,germany and ft,2004,6,int j lca 9,vehicles,significance of differences,Passenger cars,mumbai 9 seoul 9,tx 9 tokyo 9,9 ecomed publishers,worth,melbourne 9 paris,life cycle stakeholder,405 - 416},
