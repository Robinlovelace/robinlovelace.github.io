  month = jan,
  year = {2002},
  keywords = {Health Impact Assessment,evidence base,HIA},
  pages = {132-138},
  file = {/home/robin/Zotero/storage/XI9QTFMT/Joffe and Mindell - 2002 - A framework for the evidence base to support Healt.pdf;/home/robin/Zotero/storage/469TQR2X/132.html},
